@model ProductVM

@foreach (Review review in Model.Reviews)
{
    <div class="com">
        <p>@review.Name <span>@review.CreatedAt</span></p>
        <h5>@review.Message</h5>
        <div class="icons" style="color:#FFC107;">
            @for (int i = 0; i < review.Star; i++)
            {
                <i class="fa-solid fa-star"></i>
            }
        </div>
        </div>
}